<p style="color: green"><%= notice %></p>

<% content_for :title, "Tax Rates" %>

<h1>Tax Rates</h1>

<div style="margin-bottom: 1em;">
  <%= link_to "Back to Products", products_path, class: "btn btn-secondary" %>
</div>

<%= link_to "New Tax Rate", new_tax_rate_path %>

<div id="tax_rates">
